var Animal = {
    initialize: function (name, age, health, hunger) {
        this.name = name;
        this.age = age;
        this.health = health;
        this.hunger = hunger;
    }
};

var Cat = Animal.extend({
    initialize: function (name, age, health, hunger, mood, image) {
